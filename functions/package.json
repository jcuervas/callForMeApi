{
  "name": "functions",
  "scripts": {
    "lint": "tslint --project tsconfig.json",
    "build": "sh ./scripts/build.sh",
    "typesync": "typesync",
    "serve": "firebase use dev && npm run build && firebase serve --host 0.0.0.0 --only hosting,functions",
    "serve-prod": "firebase use prod && npm run build && firebase serve --host 0.0.0.0 --only hosting,functions",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "logs": "firebase functions:log",
    "config-dev": "firebase use dev",
    "config-prod": "firebase use prod",
    "deploy-prod": "sh ./scripts/deploy.sh prod",
    "deploy-dev": "sh ./scripts/deploy.sh dev",
    "deploy": "npm run deploy-dev && npm run deploy-prod",
    "migrate-dev": "sh ./scripts/migrate.sh dev",
    "migrate-prod": "sh ./scripts/migrate.sh prod",
    "migrate-local": "sh ./scripts/migrate.sh local"
  },
  "engines": {
    "node": "14"
  },
  "main": "lib/index.js",
  "dependencies": {
    "axios": "^0.21.1",
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@nestjs/platform-express": "^7.6.18",
    "cors": "^2.8.5",
    "date-fns": "^2.21.3",
    "date-fns-timezone": "^0.1.4",
    "dotenv": "^10.0.0",
    "email-templates": "^8.0.7",
    "firebase": "^8.6.3",
    "firebase-admin": "^9.9.0",
    "firebase-functions": "^3.14.1",
    "i18n": "^0.13.3",
    "mysql": "^2.18.1",
    "nodemailer": "^6.6.3",
    "nunjucks": "^3.2.3",
    "plivo": "^4.17.1",
    "stripe": "^8.149.0",
    "typeorm": "^0.2.37"
  },
  "devDependencies": {
    "@types/cors": "^2.8.12",
    "@types/email-templates": "^8.0.4",
    "@types/i18n": "^0.13.1",
    "@types/mysql": "^2.15.19",
    "@types/nodemailer": "^6.4.4",
    "@types/nunjucks": "^3.1.5",
    "firebase-functions-test": "^0.2.3",
    "tslint": "^6.1.3",
    "typescript": "^3.9.9"
  },
  "private": true
}
